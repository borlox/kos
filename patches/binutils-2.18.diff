>        echo "$timestamp" ; exit ;;
>     --version | -v )
>        echo "$version" ; exit ;;
>     --help | --h* | -h )
>        echo "$usage"; exit ;;
>     -- )     # Stop option processing
>        shift; break ;;
>     - )	# Use stdin as input.
>        break ;;
>     -* )
>        echo "$me: invalid option $1$help"
>        exit 1 ;;
> 
>     *local*)
